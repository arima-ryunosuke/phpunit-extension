{
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "name": "ryunosuke/phpunit-extension",
  "type": "library",
  "description": "PHPUnit Fluent interface and Custom assertions",
  "license": "MIT",
  "authors": [
    {
      "name": "ryunosuke",
      "email": "ryunosuke.arima@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "phpunit/phpunit": "9.*"
  },
  "require-dev": {
    "ryunosuke/functions": "1.5.*"
  },
  "autoload": {
    "psr-4": {
      "ryunosuke\\PHPUnit\\": "src"
    },
    "files": [
      "inc/ryunosuke.stub"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ryunosuke\\Test\\": "tests/Test"
    },
    "files": [
      "tests/ryunosuke.php"
    ]
  },
  "scripts": {
    "build": [
      "@test"
    ],
    "test": [
      "vendor/bin/phpunit --configuration=tests/phpunit.xml"
    ]
  }
}
